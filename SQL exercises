SELECT * FROM booksstore.books;
SELECT * FROM booksstore.books WHERE release_date > '2010' AND type = 'new' ORDER BY 'title';
SELECT title, release_date, price, type FROM booksstore.books WHERE release_date < '1970' AND type = 'used' AND price < '20';  
SELECT YEAR(order_date) AS Aasta, COUNT(*) AS 'Tellimuste arv' FROM orders GROUP BY YEAR(order_date);
